<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <!--  Any servlet can use this -->
  <!-- 
  <context-param>
  	<param-name>Test1</param-name>
  	<param-value>haha</param-value>
  </context-param>
   -->
  
  
  <!-- "Local" LoginServlet.java -->
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>com.revature.LoginServlet</servlet-class>
  	<!-- 
  	<init-param>
  		<param-name></param-name>
  		<param-value></param-value>
  	</init-param>
  	 -->
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>FrontControl</servlet-name>
  	<servlet-class>com.revature.FrontControl</servlet-class>
  	<load-on-startup>0</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>EmployeeView</servlet-name>
  	<servlet-class>com.revature.EmployeeView</servlet-class>
  	<load-on-startup>2</load-on-startup>	
  </servlet>
  
  <servlet>
  	<servlet-name>ReimbursementView</servlet-name>
  	<servlet-class>com.revature.ReimbursementView</servlet-class>
  	<load-on-startup>3</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>EmployeePending</servlet-name>
  	<servlet-class>com.revature.EmployeePending</servlet-class>
  	<load-on-startup>4</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>EmployeeResolved</servlet-name>
  	<servlet-class>com.revature.EmployeeResolved</servlet-class>
  	<load-on-startup>5</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>EmployeeUpdate</servlet-name>
  	<servlet-class>com.revature.EmployeeUpdate</servlet-class>
  	<load-on-startup>6</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>AllPending</servlet-name>
  	<servlet-class>com.revature.AllPending</servlet-class>
  	<load-on-startup>7</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>AllResolved</servlet-name>
  	<servlet-class>com.revature.AllResolved</servlet-class>
  	<load-on-startup>8</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>OneEmployee</servlet-name>
  	<servlet-class>com.revature.OneEmployee</servlet-class>
  	<load-on-startup>9</load-on-startup>
  </servlet>
  
  
  <!--  Mapping LoginServlet.java URL address -->
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>FrontControl</servlet-name>
  	<url-pattern>/FC</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EmployeeView</servlet-name>
  	<url-pattern>/empView</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ReimbursementView</servlet-name>
  	<url-pattern>/ReimbursementView</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EmployeePending</servlet-name>
  	<url-pattern>/EmployeePending</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EmployeeResolved</servlet-name>
  	<url-pattern>/EmployeeResolved</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EmployeeUpdate</servlet-name>
  	<url-pattern>/EmployeeUpdate</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AllPending</servlet-name>
  	<url-pattern>/AllPending</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AllResolved</servlet-name>
  	<url-pattern>/AllResolved</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>OneEmployee</servlet-name>
  	<url-pattern>/OneEmployee</url-pattern>
  </servlet-mapping>
  
</web-app>
